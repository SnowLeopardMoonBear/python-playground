   "source": [
    "def deco(func):\n",
    "    def wrapper(*args, **kwargs):        \n",
    "        print(\"before func\")\n",
    "        func(*args)\n",
    "        print(\"after func\")\n",
    "    return wrapper\n",
    "\n",
    "@deco\n",
    "def func_to_be_decorated(n):\n",
    "    print(\"func \"+n)\n",
    "    \n",
    "func_to_be_decorated('wrapped')"
   ]
  }
